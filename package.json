      "last 1 safari version"
    ]
  },
  "contributors": [
    "Michael Linderman <mlinderman@middlebury.edu>",
    "Christopher Andrews <candrews@middlebury.edu>"
  ],
  "license": "Apache-2.0",
  "engines": {
    "node": "12.x"
  },
  "cacheDirectories": [
    "node_modules"
  ],
  "devDependencies": {
    "eslint-config-prettier": "^6.10.0",
    "husky": "^4.2.3",
    "jest-enzyme": "^7.1.2",
    "jest-styled-components": "^7.0.0",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
